<div class="col-12">
  <li>
    <%= link_to test.title, test %>
    <%= link_to 'x', test, method: :delete, data: { confirm: 'Вы увеерены?'}  %>
    <%= link_to 'Обновить', edit_test_path(test) %>
    <p>Уровень: <%= test.level %></p>
    <p>Категория: <%= test.category.name %></p>
    <p>Кол-во вопросов: <%= test.questions.size %></p>
    <p><%= button_to 'Start', start_test_path(test), class: 'btn btn-primary' %></p>
  </li>
  <br />
</div>
