<div class="col-12">
  <li>
  <%= link_to test.title, admin_test_path(test) %>
    <%= link_to 'x', admin_test_path(test), method: :delete, data: { confirm: 'Вы уверены?'}  %>
    <%= link_to t('.update_test'), edit_admin_test_path(test) %>
    <p><%= t('tests.test.level', level: test.level) %></p>
    <p><%= t('tests.test.category', category: test.category.name) %></p>
    <p><%= t('tests.test.question_size', question_size: test.questions.size) %></p>
  </li>
  <br />
</div>
